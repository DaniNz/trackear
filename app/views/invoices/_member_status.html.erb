<% if @invoice_status.user_waiting_for_client_payment? %>
  <%= render 'invoices/invoice_status/member/waiting_client_payment', completed: false %>
<% elsif @invoice_status.user_client_paid? %>
  <%= render 'invoices/invoice_status/member/user_client_paid', completed: false %>
<% elsif @invoice_status.user_paying_in_progress? %>
  <%= render 'invoices/invoice_status/member/user_paying_in_progress', completed: false %>
<% elsif @invoice_status.user_complete? %>
  <%= render 'invoices/invoice_status/member/user_complete' %>
<% end %>

